@{
    ViewData["Title"] = "Statistics";
    Layout = "~/Views/Shared/_CoreFoodLayout.cshtml";
}

<!DOCTYPE html>
<html>
<head>

    <!-- Custom Theme files -->
    <link href="~/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <!-- Custom Theme files -->
    <script src="~/js/jquery.min.js"></script>
    <link rel="stylesheet" href="~/css/font-awesome.css">
    <!-- Custom Theme files -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--webfont-->
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>

    <link href="~/css/jqvmap.css" media="screen" rel="stylesheet" type="text/css" />


    <script src="~/js/easyResponsiveTabs.js" type="text/javascript"></script>

    <script src="~/js/main.js"></script> <!-- Resource jQuery -->
    <!-- chart -->
    <script src="~/js/Chart1.js"></script>
    <!-- //chart -->
</head>
<body>
    <div class="total-content">
        <div class="list_of_members">
            <div class="sales" style="background-color:#1abc9c">
                <div class="icon-text">
                    <h3>@ViewBag.d1</h3>
                    <p>Total Food</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="new-users" style="background-color:#2ecc71">

                <div class="icon-text">
                    <h3>@ViewBag.d2</h3>
                    <p>Total Category</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="orders" style="background-color:#e74c3c">

                <div class="icon-text">
                    <h3>@ViewBag.d3</h3>
                    <p>Fruit Count</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="visitors" style="background-color:#9b59b6">

                <div class="icon-text">
                    <h3>@ViewBag.d4</h3>
                    <p>Vegetable Count</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
    <div class="clearfix"></div>

    <div class="total-content">
        <div class="list_of_members">
            <div class="sales" style="background-color:#f39c12">
                <div class="icon-text">
                    <h3>@ViewBag.d5</h3>
                    <p>Sum Food</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="new-users" style="background-color:#d35400">

                <div class="icon-text">
                    <h3>@ViewBag.d6</h3>
                    <p>Legumes Count</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="orders" style="background-color:#c0392b">

                <div class="icon-text">
                    <h3>@ViewBag.d7</h3>
                    <h4> @ViewBag.d7_1 Box</h4>
                    <p>Max Stock Food</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="visitors" style="background-color:#7f8c8d">

                <div class="icon-text">
                    <h3>@ViewBag.d8</h3>
                    <h4> @ViewBag.d8_1 Box</h4>
                    <p>Min Stock Food</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>

    <div class="clearfix"></div>

    <div class="total-content">
        <div class="list_of_members">
            <div class="sales" style="background-color:#40407a">
                <div class="icon-text">
                    <h3>@ViewBag.d9 ₺</h3>
                    <p>Food Price Avg</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="new-users" style="background-color:#34ace0">

                <div class="icon-text">
                    <h3>@ViewBag.d10</h3>
                    <p>Sum Fruit</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="orders" style="background-color:#ccae62">

                <div class="icon-text">
                    <h3>@ViewBag.d11</h3>
                    <p>Sum Vegetables</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="visitors" style="background-color:#218c74">

                <div class="icon-text">
                    <h3>@ViewBag.d12</h3>
                    <h4>@ViewBag.d12_1 ₺</h4>
                    <p>Max Price Food</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</body>
</html>

