
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">
<head>
    <title>Core Project:Foods</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/CoreAndFood.styles.css" asp-append-version="true" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"></script>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
</head>
<body>

    <nav class="navbar navbar-expand-sm navbar-dark bg-dark">
        <div class="container-fluid">
            <a class="navbar-brand" >Core And Food</a>
            <ul class="navbar-nav me-auto">
                <li class="nav-item">
                    <a asp-action="Index" asp-controller="Category" class="nav-link text-light">Category</a>
                </li>
                <li class="nav-item ">
                    <a class="nav-link text-light" asp-action="Index" asp-controller="Food" >Food</a>
                </li>
@*                <li class="nav-item ">
                    <a class="nav-link text-light" asp-action="Index" asp-controller="Chart" >Google Pie Chart</a>
                </li> 
                <li class="nav-item ">
                    <a class="nav-link text-light" asp-action="ColIndex" asp-controller="Chart" >Google Column Chart</a>
                </li> *@
                <li class="nav-item ">
                    <a class="nav-link text-light" asp-action="FoodIndex" asp-controller="Chart" >Google Dynamic Chart</a>
                </li>    
                <li class="nav-item ">
                    <a class="nav-link text-light" href="#">Popup</a>
                </li>               
                <li class="nav-item ">
                    <a class="nav-link text-light" asp-action="Statistics" asp-controller="Chart">Statistics</a>
                </li>
 
            </ul>
            <ul class="navbar-nav justify-content-end">
                <li class="nav-item ">
                    <a class="nav-link text-light" href="#">Settings</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-light" asp-action="LogOut" asp-controller="Login">Log Out</a>
                </li>
            </ul>
        </div>
    </nav>

    <div class="container-fluid mt-3">
        @RenderBody()
    </div>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
</body>
</html>
